prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
rtm_prefix=@prefix@
rtm_exec_prefix=@exec_prefix@
rtm_cxx=@CXX@
rtm_cflags=@CXXFLAGS@ -I${prefix}/include -I@rtm_includedir@ -I@rtm_includedir@/rtm/idl
rtm_libs=@LDFLAGS@ -L${prefix}/lib @LIBS@ -lRTC -lcoil
rtm_libdir=@pkglibdir@
rtm_version=@RTM_VERSION@
rtm_orb=@ORB@
rtm_idlc=@IDLC@
rtm_idlflags=@IDLFLAGS@

Name: @PACKAGE@
Description: @RTM_DESCRIPTION@
Version: @RTM_VERSION@
Requires: 
Libs: ${rtm_libs}
Cflags: ${rtm_cflags}
